{
  "name": "python-eel-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite build && python src/app.py",
    "watch": "nodemon --exec 'vite build && python src/app.py' --watch src --ext py,html,js,css,vue",
    "build": "python -m eel src/app.py web --onefile",
    "py-install": "pip install -r requirements.txt",
    "preview": "vite preview"
  },
  "dependencies": {
    "nodemon": "^3.1.3",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "vite": "^5.2.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
